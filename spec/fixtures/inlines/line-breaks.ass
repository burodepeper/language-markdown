# http://spec.commonmark.org/0.22/#hard-line-breaks

@580
"foo  "+
"baz" {
  source.text.md {
    "foo"
    "  ": line-break.constant.md
    "baz"
  }
}

@581
"foo\"+
"baz" {
  source.text.md {
    "foo"
    "\": line-break.constant.md
    "baz"
  }
}

@582
"foo       "+
"baz" {
  source.text.md {
    "foo     "
    "  ": line-break.constant.md
    "baz"
  }

}

