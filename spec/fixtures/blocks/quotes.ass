# http://spec.commonmark.org/0.22/#block-quotes

@178
'> # Foo'+
'> bar'+
'> baz' {
  source.text.md {
    quote.markup.md {
      '>': punctuation.md
      ' '
      heading.support.markup.md {
        heading-1.md {
          '#': punctuation.md
          ' '
          'Foo'
        }
      }
      '>': punctuation.md
      ' '
      'bar'
      '>': punctuation.md
      ' '
      'baz'
    }
  }
}

