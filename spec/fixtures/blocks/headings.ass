# http://spec.commonmark.org/0.22/#example-27
@27
'# Heading' {
  source.text.md {
    heading.support.markup.md {
      heading-1.md {
        '#': punctuation.md
        ' '
        'Heading'
      }
    }
  }
}

# http://spec.commonmark.org/0.22/#example-28
@28
'####### foo' {
  '####### foo': source.text.md
}

# http://spec.commonmark.org/0.22/#example-29
@29a
'#5 bolt' {
  source.text.md {
    reference.gfm.variable.md {
      '#': punctuation.md
      '5': issue.gfm.md
    }
    ' bolt'
  }
}

@29b
'#foobar' {
  '#foobar': source.text.md
}

# http://spec.commonmark.org/0.22/#example-30
@30
'\## foo' {
  source.text.md {
    '\#': escape.constant.md
    '# foo'
  }
}

# http://spec.commonmark.org/0.22/#example-31
@31
'# foo *bar* \*baz\*' {
  source.text.md {
    heading.support.markup.md {
      heading-1.md {
        '#': punctuation.md
        ' '
        'foo '
        emphasis.italic.markup.md {
          '*': punctuation.md
          'bar'
          '*': punctuation.md
        }
        ' '
        '\*': escape.constant.md
        'baz'
        '\*': escape.constant.md
      }
    }
  }
}

