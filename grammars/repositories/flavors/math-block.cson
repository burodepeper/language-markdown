key: 'math-block'
patterns: [
  {
    name: 'block.math.markup.md'
    begin: '(\\${2})'
    end: '(\\${2})(?:.*)'
    patterns: [{ include: 'text.tex.latex' }]
    beginCaptures:
      1: name: 'punctuation.md'
    endCaptures:
      1: name: 'punctuation.md'
  }

  {
    name: 'block.math.markup.md'
    begin: '\\\\\\['
    end: '\\\\\\]'
    patterns: [{ include: 'text.tex.latex' }]
    beginCaptures:
      1: name: 'punctuation.md'
    endCaptures:
      1: name: 'punctuation.md'
  }

  {
    name: 'block.math.markup.md'
    begin: '\\\\begin\\{(equation\\*?)\\}'
    end: '\\\\end\\{\\1\\}'
    patterns: [{ include: 'text.tex.latex' }]
    beginCaptures:
      1: name: 'punctuation.md'
    endCaptures:
      1: name: 'punctuation.md'
  }
]
