key: 'front-matter'
patterns: [
  {
    begin: '\\A---$'
    beginCaptures:
      0: name: 'hr.constant.markup.md'
    end: '^(---|\\.\\.\\.)$'
    endCaptures:
      1: name: 'hr.constant.markup.md'
    name: 'front-matter.yaml.source.md'
    patterns: [{ include: 'source.yaml' }]
  }
  {
    begin: '\\A\\+\\+\\+$'
    beginCaptures:
      0: name: 'hr.constant.markup.md'
    end: '^\\+\\+\\+$'
    endCaptures:
      0: name: 'hr.constant.markup.md'
    name: 'front-matter.toml.source.md'
    patterns: [{ include: 'source.toml' }]
  }
]
